{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "run-s migrate-dev db-seed && next dev",
    "dx:prisma-studio": "pnpm prisma-studio",
    "dx": "run-p dx:* --print-label",
    "dev": "npm-run-all --parallel start-dev typesafe-i18n",
    "prebuild": "run-s generate migrate",
    "build": "next build",
    "start-dev": "next dev",
    "start": "next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "postinstall": "prisma generate",
    "typesafe-i18n": "typesafe-i18n"
  },
  "packageManager": "pnpm@8.5.0",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/react-query": "^4.32.0",
    "@tanstack/react-query-devtools": "^4.32.0",
    "@trpc/client": "^10.36.0",
    "@trpc/next": "^10.36.0",
    "@trpc/react-query": "^10.36.0",
    "@trpc/server": "^10.36.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "next": "^13.4.8",
    "next-pwa": "^5.6.0",
    "next-superjson": "^0.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.13.1",
    "typesafe-i18n": "^5.26.0",
    "ui": "workspace:*",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "autoprefixer": "^10.4.13",
    "critters": "^0.0.20",
    "daisyui": "^3.5.0",
    "eslint": "^8.40.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-next": "^13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "prettier": "^2.8.8",
    "prisma": "^5.1.1",
    "start-server-and-test": "^1.12.0",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.3.0",
    "tsconfig": "workspace:*",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  }
}
